{
    "name": "lendsql-be-test",
    "version": "1.0.0",
    "description": "A monolithic architecture project that allows user to create account , make a deposit, withdraw from the account and credit other users account",
    "main": ".eslintrc.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "start": "node ./dist/src/server.js",
        "start:dev": "tsc-watch --onSuccess \"node -r dotenv/config ./dist/src/server.js\"",
        "migrate": "node migrate:latest",
        "make": "knex migrate:make",
        "seed": "knex seed:make",
        "build": "rm -rf dist && tsc",
        "format": "prettier --config .prettierrc.js --write  \"**/*.{ts,tsx}\"",
        "format:check": "prettier --config .prettierrc.js --write './**/*.{js,ts,json,yml,yaml}'",
        "lint": "eslint --ignore-path .eslintignore --ext .ts,.js .",
        "lint:fix": "eslint --ignore-path .eslintignore --ext .ts,.js --fix .",
        "test:all": "cross-env NODE_ENV=test jest --verbose --runInBand --collectCoverage=true ./tests",
        "test": "cross-env NODE_ENV=test jest --verbose --runInBand --collectCoverage=true ./tests/unit",
        "test-integration": "cross-env NODE_ENV=test jest --verbose --runInBand --collectCoverage=true ./tests/integration"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.1",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "crypto": "^1.0.1",
        "crypto-js": "^4.1.1",
        "dayjs": "^1.11.5",
        "express": "^4.18.1",
        "express-http-context": "^1.2.4",
        "express-validator": "^6.14.2",
        "git": "^0.1.5",
        "helmet": "^4.6.0",
        "http-graceful-shutdown": "^3.1.3",
        "http-status": "^1.5.3",
        "joi": "^17.6.1",
        "js-base64": "^3.7.3",
        "jsonwebtoken": "^8.5.1",
        "knex": "^2.3.0",
        "launchdarkly-node-server-sdk": "^6.1.0",
        "lodash": "^4.17.21",
        "module-alias": "^2.2.2",
        "moment": "^2.29.4",
        "mongoose": "^6.2.10",
        "multer": "^1.4.4",
        "mysql": "^2.18.1",
        "nodemailer": "^6.8.0",
        "pg": "^8.7.1",
        "pino": "^8.6.1",
        "randomstring": "^1.2.3",
        "reflect-metadata": "^0.1.13",
        "source-map-support": "^0.5.19",
        "swagger-parser": "^10.0.2",
        "ts-loader": "^9.4.2",
        "uuid": "^8.3.2",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.2",
        "@types/connect-datadog": "^0.0.6",
        "@types/cookie-parser": "^1.4.3",
        "@types/crypto-js": "^4.1.1",
        "@types/errorhandler": "1.5.0",
        "@types/express": "^4.17.11",
        "@types/express-session": "^1.17.5",
        "@types/jest": "^26.0.24",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/lodash": "^4.14.181",
        "@types/module-alias": "^2.0.0",
        "@types/node": "^16.4.10",
        "@types/nodemailer": "^6.4.6",
        "@types/pg": "^8.6.1",
        "@types/randomstring": "^1.1.8",
        "@types/supertest": "^2.0.10",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.23.0",
        "cross-env": "^7.0.2",
        "dockest": "^2.0.2",
        "dotenv": "^16.0.3",
        "eslint": "^7.29.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "jest-extended": "^0.11.5",
        "lint-staged": "^11.2.6",
        "prettier": "^2.3.0",
        "supertest": "^6.1.6",
        "ts-jest": "^26.5.0",
        "ts-node": "^10.8.0",
        "ts-node-dev": "^2.0.0-0",
        "tsc-watch": "^4.5.0",
        "tslib": "^2.3.0",
        "typescript": "^4.9.4"
    }
}
